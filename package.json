{
  "name": "node-scripts-docs",
  "version": "0.0.7",
  "bin": {
    "nsd": "output/node-scripts-docs.js"
  },
  "scripts": {
    "build": "tsc",
    "docs:build": "docker run --rm -it -p 8000:8000 -v ${PWD}:/docs squidfunk/mkdocs-material build --site-dir=public",
    "docs:scripts": "yarn nsd-debug --include-locals",
    "docs:serve": "docker run --rm -it -p 8000:8000 -v ${PWD}:/docs squidfunk/mkdocs-material",
    "npm:publish": "yarn build && npm publish",
    "npm:publish:patch": "yarn build && npm version patch && npm publish",
    "nsd-debug": "yarn build && node output/node-scripts-docs.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "elapsed-time": "0.0.1",
    "globby": "11.1.0",
    "marked": "4.0.12",
    "minimist": "1.2.5",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/marked": "4.0.2",
    "@types/minimist": "1.2.2",
    "@types/node": "17.0.10",
    "@types/prettier": "2.4.4",
    "@types/prettier-package-json": "2.1.1",
    "@yarnpkg/sdks": "2.6.0",
    "eslint": "8.8.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.6",
    "prettier": "2.5.1",
    "prettier-package-json": "2.6.3",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-sh": "0.8.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "packageManager": "yarn@3.1.1"
}
